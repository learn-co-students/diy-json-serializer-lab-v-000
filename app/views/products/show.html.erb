<h1 class="name"><%= @product.name %></h1>
<h2 class="description"><%= @product.description %></h2>
<p class="inventory">Inventory: <%= @product.inventory %></p>
<p class="price">Price: <%= @product.price %></p>
<%= link_to "Next Product",product_path(@product), id: "js-next", data: {id: @product.id} %>
