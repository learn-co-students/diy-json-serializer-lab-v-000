




<%= form_for @user do |f| %>

	<%= f.label :username %>
	<%= f.text_field :username, class: "form-control log-label" %><br>

	<%= f.label :password %>
	<%= f.password_field :password, class: "form-control log-label", required: true %><br>

	<%= f.label :email %>
	<%= f.text_field :email, class: "form-control log-label", required: true %><br>

	<%= f.submit "Create User", class: "btn btn-primary" %>
<% end %>

